{
  "ConnectionStrings": {
    "ConferenceAttendeeDatabaseConnection": "Host=localhost;Port=5432;Database=ConferenceAttendeeDb;Username=postgres;Password=12345"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": { "serverUrl": "/logs/log.txt" }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341/" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "ApplicationName": "Conference Attendees API"
    }
  },
  "AllowedHosts": "*"
}

